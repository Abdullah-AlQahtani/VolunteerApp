<%= form_for @user do |f| %>
      <div class="field">
  <%= f.label :name %><br />
  <%= f.text_field :name, autocomplete: "name" %>
</div>

<br>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

<br>


  <div class="field">
  <%= f.label :gender %><br />
  <%= f.select :gender, ['Male', 'Female']%>
</div>

<br>

<div class="field">
  <%= f.label :dob, "Date of Birth" %><br />
  <%= f.date_select :dob, autocomplete: "dob", order: [:day, :month, :year], prompt: { day: 'Select day', month: 'Select month', year: 'Select year' }, start_year: Date.today.year , end_year: Date.today.year - 100 %>
</div>

<br>

<div class="field">
  <%= f.label :city %><br />
  <%= f.text_field :city, autocomplete: "city" %>
</div>

<br>
  <div class="actions">
    <%= f.submit %>
  </div>
  <br>

<% end %>