<h1>Create a new Organization </h1>

<%= form_for @organization do |f| %>
      <div class="field">
  <%= f.label :name %><br />
  <%= f.text_field :name, autocomplete: "name" %>
</div>

<br>

<div class="field">
  <%= f.label :website %><br/>
  <%= f.text_field :website %>
</div>

<br>

<div class="field">
  <%= f.label :address %><br />
  <%= f.text_field :address %>
</div>

<br>
  <div class="actions">
    <%= f.submit %>
  </div>
  <br>

<% end %>