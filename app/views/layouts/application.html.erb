<!DOCTYPE html>
<html>
  <head>
    <title>IVolunteer</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
   
     <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"> </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
    
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

      <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.7.0/animate.min.css">

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
   
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <center>
  <img class="center" src="http://kingdomvt.com/wp-content/uploads/2017/10/admin-ajax.php_.png">
  </center>
  <br>
  <nav class="navbar navbar-expand-lg navbar-light">
  <div class="collapse navbar-collapse" id="navbarNav">
    <ul class="navbar-nav">
      <li class="nav-item active">
      </li>
       <li class="nav-item">
        <%= link_to 'Home', root_path, class: "nav-link navbar-menu" %>
      </li>
      <% if user_signed_in? %>
      <li class="nav-item">
        <%= link_to 'Profile', user_path(current_user), class: "nav-link navbar-menu" %>
      </li>
       <li class="nav-item">
        <%= link_to 'Log Out', destroy_user_session_path, method: :delete, class: "nav-link navbar-menu" %>
      </li>
      <% else %>
       <li class="nav-item">
        <%= link_to 'Login / Logout', new_user_session_path, class: "nav-link navbar-menu" %>
        <li>
      <% end %>
    </ul>
  </div>
</nav>
    <%= yield %>
  </body>
</html>
